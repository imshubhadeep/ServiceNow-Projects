        <script><![CDATA[var MassAssigneeChangeAjax = Class.create();
MassAssigneeChangeAjax.prototype = Object.extendsObject(AbstractAjaxProcessor, {
 updateAssignee: function() {
   // required params: sysparm_from_user (sys_id), sysparm_to_user (sys_id)
   var fromSys = this.getParameter('sysparm_from_user');
   var toSys = this.getParameter('sysparm_to_user');
   if (!fromSys || !toSys) {
     return this._encode({error: 'missing_parameters'});
   }
   if (fromSys == toSys) {
     return this._encode({error: 'same_user'});
   }
   // Security check: ensure caller has update rights on incident
   var caller = this.getUser(); // GlideSystem user object
   var canUpdate = gs.hasRole('admin') || gs.hasRole('itil'); // change as needed
   // Better approach: check ACL programmatically on incident table (omitted for brevity)
   if (!canUpdate) {
     return this._encode({error: 'insufficient_privileges'});
   }
   // Query incidents assigned to fromSys and update in batches
   var batchSize = 200; // adjust per performance
   var processed = 0;
   var gr = new GlideRecord('incident');
   gr.addQuery('assigned_to', fromSys);
   gr.query();
   var sysIDsToUpdate = [];
   while (gr.next()) {
     sysIDsToUpdate.push(gr.getValue('sys_id'));
     if (sysIDsToUpdate.length >= batchSize) {
       processed += this._updateBatch(sysIDsToUpdate, toSys);
       sysIDsToUpdate = [];
     }
   }
   if (sysIDsToUpdate.length > 0) {
     processed += this._updateBatch(sysIDsToUpdate, toSys);
   }
   return this._encode({updated: processed});
 },
 _updateBatch: function(sysIdArray, toSys) {
   // Use GlideRecord and update each record. Could be optimized via server-side engine if available.
   var n = 0;
   var gr = new GlideRecord('incident');
   gr.addQuery('sys_id', 'IN', sysIdArray.join(','));
   gr.query();
   while (gr.next()) {
     // Optionally: write audit / journal entry in work_notes
     gr.assigned_to = toSys;
     try {
       gr.update();
       n++;
     } catch (e) {
       gs.error('Failed updating incident ' + gr.getValue('sys_id') + ': ' + e);
     }
   }
   return n;
 },
 _encode: function(obj) {
   return new JSON().encode(obj);
 },
 type: 'MassAssigneeChangeAjax'
});]]></script>
